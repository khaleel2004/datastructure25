#include <stdio.h>
int main() 
{
    int i;
    printf("The first 10 natural numbers are:\n");
    for (i = 1; i <= 10; i++) 
	{
        printf("%d ", i);
    }
    printf("\n");
	printf("Odd numbers: ");
    for (i = 1; i <= 10; i++) 
	{
        if (i % 2 != 0) 
		{
            printf("%d ", i);
        }
    }
    printf("\n");
	printf("Even numbers: ");
    for (i = 1; i <= 10; i++) 
	{
        if (i % 2 == 0) 
		{
            printf("%d ", i);
        }
    }
    printf("\n");
	return 0;
}
